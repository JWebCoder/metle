    "url": "https://github.com/JWebCoder/metle/issues"
  },
  "homepage": "https://github.com/JWebCoder/metle#readme",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.37",
    "codecov": "^3.5.0",
    "jest": "^25.5.4",
    "rimraf": "^2.7.1",
    "ts-jest": "^25.4.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3"
  }
