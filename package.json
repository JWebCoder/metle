  "description": "memory storage with ttl and reset after request quantity",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">=9.11.1"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
