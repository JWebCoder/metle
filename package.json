  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "codecov": "^3.5.0",
    "jest": "^25.5.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
